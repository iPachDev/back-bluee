{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/back-bluee/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/back-bluee/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/back-bluee",
  "projects": {
    "back-bluee": {
      "type": "application",
      "root": "apps/back-bluee",
      "entryFile": "main",
      "sourceRoot": "apps/back-bluee/src",
      "compilerOptions": {
        "tsConfigPath": "apps/back-bluee/tsconfig.app.json"
      }
    },
    "users": {
      "type": "application",
      "root": "apps/users",
      "entryFile": "main",
      "sourceRoot": "apps/users/src",
      "compilerOptions": {
        "tsConfigPath": "apps/users/tsconfig.app.json"
      }
    },
    "talent": {
      "type": "application",
      "root": "apps/talent",
      "entryFile": "main",
      "sourceRoot": "apps/talent/src",
      "compilerOptions": {
        "tsConfigPath": "apps/talent/tsconfig.app.json"
      }
    },
    "organizations": {
      "type": "application",
      "root": "apps/organizations",
      "entryFile": "main",
      "sourceRoot": "apps/organizations/src",
      "compilerOptions": {
        "tsConfigPath": "apps/organizations/tsconfig.app.json"
      }
    }
  }
}